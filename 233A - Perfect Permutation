#include <iostream>
#include <algorithm>

using namespace std;

int main()
{
    int i;

    cin >> i;

    if(i%2==1){
        cout << -1<< endl;
    }

    else{

        cout << "2 1";

        for (int j = 3; j < i; j+=2)
        {
            cout << " " << j + 1 << " " << j;
        }
        
    }

   return 0;
}
